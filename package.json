{
  "name": "fontawesome-pro-mcp",
  "version": "1.0.0",
  "description": "Font Awesome Pro MCP Server for icon search and retrieval",
  "main": "dist/server.js",
  "type": "module",
  "scripts": {
    "dev": "tsx watch src/server.ts",
    "dev:mcp": "tsx watch src/mcp-server.ts",
    "build": "tsc",
    "build:mcp": "tsc src/mcp-server.ts --outDir dist --module es2022 --target es2022 --moduleResolution node",
    "start": "node dist/server.js",
    "start:mcp": "node dist/mcp-server.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "clean": "rm -rf dist"
  },
  "keywords": [
    "mcp",
    "fontawesome",
    "icons",
    "server"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@fortawesome/fontawesome-pro": "^6.7.2",
    "@fortawesome/free-brands-svg-icons": "^6.5.0",
    "@fortawesome/free-regular-svg-icons": "^6.5.0",
    "@fortawesome/free-solid-svg-icons": "^6.5.0",
    "@fortawesome/pro-duotone-svg-icons": "^6.5.0",
    "@fortawesome/pro-light-svg-icons": "^6.5.0",
    "@fortawesome/pro-regular-svg-icons": "^6.5.0",
    "@fortawesome/pro-solid-svg-icons": "^6.5.0",
    "@fortawesome/pro-thin-svg-icons": "^6.5.0",
    "@modelcontextprotocol/sdk": "^1.0.0",
    "express": "^4.18.0",
    "fuse.js": "^7.0.0",
    "js-yaml": "^4.1.0",
    "zod": "^3.22.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.0",
    "@types/jest": "^29.5.14",
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^20.0.0",
    "@types/supertest": "^6.0.3",
    "jest": "^29.7.0",
    "supertest": "^6.3.4",
    "ts-jest": "^29.4.1",
    "tsx": "^4.0.0",
    "typescript": "^5.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
